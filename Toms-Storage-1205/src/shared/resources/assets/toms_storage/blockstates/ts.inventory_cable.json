{
    "multipart": [
    	{	"apply": { "model": "toms_storage:block/cable_base" } },
        {   "when": { "north": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "uvlock": true }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "x": -90, "uvlock": true }
        },
        {   "when": { "down": "false" },
            "apply": { "model": "toms_storage:block/cable_closed", "x": 90, "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "x": -90, "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "toms_storage:block/cable_open", "x": 90, "uvlock": true }
        }
    ]
}
